{
  "name": "demo-sync-shipengine-to-moltin",
  "alias": "particular-demo-sync-shipengine-to-moltin.now.sh",
  "env": {
    "NODE_ENV": "production",
    "MOLTIN_CLIENT_ID": "@demo-moltin-client-id",
    "MOLTIN_CLIENT_SECRET": "@demo-moltin-client-secret"
  },
  "routes": [{ "src": "/(.*)", "dest": "/src" }],
  "builds": [
    {
      "src": "src/*.js",
      "use": "@now/node"
    }
  ]
}
